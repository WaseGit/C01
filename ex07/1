#include <stdio.h>
#include <unistd.h>

void	ft_rev_int_tab(int *tab, int size)
{
	int i;
	int a;
	int *tab; 
	
	tab[5] = {1, 5, 6, 40, 75};
	i = 0;
	while (i < (size / 2))
	{
		a = tab[i];
		tab[i] = tab[size - i - 1];
		tab[size -i -1] = a;
		write(1, &tab[i], 1);
		i++;
	}
}

int	main	(void)
{
	ft_rev_int_tab(*tab, 10);
	return 0; 
}
